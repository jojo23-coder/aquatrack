        "id": "t_water_fill_dose",
        "args": {
          "type": "gh"
        },
        "when": "products.gh_remineralizer.selected == true && !products.gh_kh_remineralizer.selected"
      },
      {
        "id": "t_water_fill_dose",
        "args": {
          "type": "kh"
        },
        "when": "products.kh_buffer.selected == true && !products.gh_kh_remineralizer.selected"
      },
      "t_water_fill_tank"
    ],
    "duration_days": 1
  },
